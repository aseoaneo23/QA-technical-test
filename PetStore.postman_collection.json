		{
			"name": "Create new user",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Define el cuerpo de solicitud JSON que esperas enviar\r",
							"var requestBody = {\r",
							"    \"id\": 1,\r",
							"    \"username\": \"AntonioSeoane\",\r",
							"    \"firstName\": \"Antonio\",\r",
							"    \"lastName\": \"Seoane\",\r",
							"    \"email\": \"miemail@mail.com\",\r",
							"    \"password\": \"12345\",\r",
							"    \"phone\": \"1234\",\r",
							"    \"userStatus\": 0\r",
							"};\r",
							"\r",
							"// Define una función auxiliar para validar el formato de correo electrónico\r",
							"function isValidEmail(email) {\r",
							"    // Utiliza una expresión regular para validar el formato del correo electrónico\r",
							"    \r",
							"    var emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\r",
							"    return emailRegex.test(email);\r",
							"}\r",
							"\r",
							"// Usa pm.expect() junto con pm.validate() para validar el cuerpo de la solicitud\r",
							"pm.test(\"El cuerpo de la solicitud es válido\", function () {\r",
							"    pm.expect(requestBody.id).to.be.a('number');\r",
							"    pm.expect(requestBody.id).to.be.above(0);\r",
							"\r",
							"    pm.expect(requestBody.username).to.be.a('string');\r",
							"    pm.expect(requestBody.username).to.not.be.empty;\r",
							"\r",
							"    pm.expect(requestBody.firstName).to.be.a('string');\r",
							"    pm.expect(requestBody.firstName).to.not.be.empty;\r",
							"\r",
							"    pm.expect(requestBody.lastName).to.be.a('string');\r",
							"    pm.expect(requestBody.lastName).to.not.be.empty;\r",
							"\r",
							"    pm.expect(requestBody.email).to.be.a('string');\r",
							"    pm.expect(isValidEmail(requestBody.email)).to.be.true;\r",
							"\r",
							"    pm.expect(requestBody.password).to.be.a('string');\r",
							"    pm.expect(requestBody.password).to.not.be.empty;\r",
							"\r",
							"    pm.expect(requestBody.phone).to.be.a('string');\r",
							"    pm.expect(requestBody.phone).to.not.be.empty;\r",
							"\r",
							"    pm.expect(requestBody.userStatus).to.be.a('number');\r",
							"    pm.expect(requestBody.userStatus).to.be.within(0, 1); // or any other valid range\r",
							"\r",
							"    // Si llegamos aquí, significa que todas las validaciones han pasado\r",
							"    // No es necesario devolver nada explícitamente, pero podemos imprimir un mensaje de éxito si lo deseas\r",
							"    console.log(\"El cuerpo de la solicitud es válido.\");\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
